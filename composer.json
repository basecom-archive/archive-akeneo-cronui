{
  "name": "basecom/akeneo-cron-ui",
  "description": "This bundle provides an UI for managing Cronjobs in Akeneo",
  "type": "symfony-bundle",
  "require": {
    "akeneo/pim-community-dev": "3.2.x-dev@dev",
    "tightenco/collect": "^5.7",
    "dragonmantank/cron-expression": "^2.2",
    "cocur/background-process": "^0.7.0",
    "akeneo-labs/custom-entity-bundle": "3.0.*"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Jordan Kniest",
      "email": "j.kniest@basecom.de"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Basecom\\Bundle\\CronUiBundle\\": "src/Basecom/Bundle/CronUiBundle/"
    }
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.13"
  }
}
